<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

    <!-- Drug business object -->
    <bean id="drugGroupBo" class="org.mskcc.cbio.oncokb.bo.impl.DrugBoImpl">
        <property name="dao" ref="drugGroupDao"/>
    </bean>

    <!-- Drug Data Access Object -->
    <bean id="drugGroupDao" class="org.mskcc.cbio.oncokb.dao.impl.DrugDaoImpl">
        <property name="sessionFactory" ref="sessionFactory"></property>
    </bean>


</beans>
